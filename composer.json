{
  "name": "fond-of-spryker/search-elasticsearch",
  "description": "Extends spryker search elasticsearch module.",
  "license": "MIT",
  "authors": [
    {
      "name": "Daniel Rose",
      "email": "daniel-rose@gmx.de"
    },
    {
      "name": "Felix Pfister",
      "email": "felix@pfister.de"
    }
  ],
  "require": {
    "php": ">=7.2",
    "spryker/search-elasticsearch": "^1.0.0"
  },
  "require-dev": {
    "codeception/codeception": "^2.4",
    "fond-of-codeception/spryker": "^1.0",
    "php-coveralls/php-coveralls": "^2.1",
    "phpro/grumphp": "^0.14",
    "sebastian/phpcpd": "^4.0",
    "spryker/code-sniffer": "^0.12"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "FondOfSpryker\\": "src/FondOfSpryker/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "FondOfSpryker\\": "tests/FondOfSpryker/",
      "Generated\\": "src/Generated/"
    }
  }
}
